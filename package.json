{
  "name": "movie-database-explorer",
  "version": "1.0.0",
  "private": true,
  "description": "Movie Database Explorer - Aplikasi React interaktif untuk mencari film, melihat detail, dan menyimpan favorit (OMDb API).",
  "keywords": [
    "react",
    "omdb",
    "movie",
    "database",
    "search",
    "favorite"
  ],
  "author": "Gian Ivander",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "npm start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "."
}
